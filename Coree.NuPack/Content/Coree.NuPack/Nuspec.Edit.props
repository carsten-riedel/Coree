<Project>

  <!-- https://learn.microsoft.com/en-us/nuget/reference/msbuild-targets -->
  <!-- https://learn.microsoft.com/en-us/cpp/build/reference/common-macros-for-build-commands-and-properties?view=msvc-170 -->
  <!-- https://learn.microsoft.com/en-us/visualstudio/msbuild/msbuild-reserved-and-well-known-properties?view=vs-2022 -->
  <!-- https://learn.microsoft.com/en-us/dotnet/core/project-sdk/msbuild-props -->
  <!-- https://github.com/NuGet/NuGet.Client/blob/4.3.0.4202/src/NuGet.Core/NuGet.Build.Tasks.Pack.Library/Pack.targets -->

  <PropertyGroup>

    <Coree-Nuspec-VersionMajor>0</Coree-Nuspec-VersionMajor>
    <Coree-Nuspec-VersionMinor>1</Coree-Nuspec-VersionMinor>
    
    <Authors></Authors>
    <Copyright></Copyright>
    <Company></Company>
    <Description>
      <![CDATA[
Package Description
	]]>
    </Description>
    <PackageTags></PackageTags>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl></RepositoryUrl>
    <RepositoryType></RepositoryType>
    <RepositoryBranch></RepositoryBranch>

    <!-- Uses Enviroment Username for <Authors> if <Authors> is not set. -->
    <Coree-Nuspec-AutoAuthors>true</Coree-Nuspec-AutoAuthors>

    <!-- Uses <Authors> for <Company> if <Company> is not set. -->
    <Coree-Nuspec-AutoCompany>true</Coree-Nuspec-AutoCompany>

    <!-- Uses <Company> for <Authors> if <Company> is set. -->
    <Coree-Nuspec-UseCompanyAuthors>true</Coree-Nuspec-UseCompanyAuthors>

    <!-- Uses <Company> for the <Copyright> if <Copyright> is not set and <Company> is set. Copyright © YYYY $(Company) -->
    <Coree-Nuspec-AutoCopyright>true</Coree-Nuspec-AutoCopyright>

  </PropertyGroup>

  <ItemGroup>
    <Coree-Nuspec-CopyShares Include="C:\temp" />
  </ItemGroup>

</Project>